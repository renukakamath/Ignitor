pragma solidity >= 0.5.0 < 0.7.0;
contract Evuniversal{
 
struct payment{

	// State variables
	uint payment_id;
	uint requested_id;
	string requestedfor;
	string amount;
	
	string date;
		string name;
	

}

 
payment []m;



function add_payment(uint payment_id,uint requested_id,string memory requestedfor,string memory amount,string memory date,string memory name) public{
	payment memory m1=payment(payment_id,requested_id,requestedfor,amount,date,name);
	m.push(m1);
}

struct user{

	// State variables
	uint user_id;
	uint login_id;
	string first_name;
	string last_name;
	string place;
	string phone;
	string email;
	string latitude;
	string longitude;
	

}

 
user []u;



function add_user(uint user_id,uint login_id,string memory first_name,string memory last_name,string memory place,string memory phone,string memory email,string memory latitude,string memory longitude) public{
	user memory u1=user(user_id,login_id,first_name,last_name,place,phone,email,latitude,longitude);
	u.push(u1);
}



struct wallet{

	// State variables
	uint wallet_id;
	uint user_id;
	string amount;
	
	
	

}

 
wallet []w;



function add_wallet(uint wallet_id,uint user_id,string memory amount) public{
	wallet memory w1=wallet(wallet_id,user_id,amount);
	w.push(w1);
}



}




